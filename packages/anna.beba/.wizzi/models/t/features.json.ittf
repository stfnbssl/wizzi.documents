$group
    
    useWebFontLoader false
    { webFontLoader
        [ providers
            { 
                name "google" 
                [ families
                    "Aclonica"
                    "Acme"
                    "Roboto"
    
    useGoogleFonts true
    { googleFonts
        families "Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons"
        $$ families "Alice|Open+Sans:400,300,700"
    
    useFontAwesome true
    { fontAwesome
        version "latest"
    
    useBootstrap true
    { bootstrap
        $$ slider require version 3. see https://github.com/seiyria/bootstrap-slider/issues/689
        version "4.3.1"
        $$ version "3.3.7"
        $$ useSlider true
    
    useMaterialKit false
    { materialKit
        assetsBaseUri "http://localhost:5000/static/demo/material-kit"


    useDisqus false
    { disqus
        site "xxx.disqus.com"

    useGoogleTagmanager false
    { googleTagmanager
        trackerID "UA-0000000-0"

    useGoogleAnalytics false
    { googleAnalytics
        trackerID "GA-000000000-0"
